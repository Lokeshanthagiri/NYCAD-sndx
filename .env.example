# Backend environment variables
PORT=4000
NODE_ENV=development
# Postgres connection (used by pg)
PGHOST=postgres
PGPORT=5432
PGUSER=postgres
PGPASSWORD=postgres
PGDATABASE=fhv
# Socrata API (optional app token)
SOCRATA_APP_TOKEN=
# Cron schedule for daily sync (default 3 AM UTC)
CRON_SCHEDULE=0 3 * * *
